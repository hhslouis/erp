	<queryString>
		<![CDATA[
select (case when a.ide_copag = (select cast(valor_para as integer) from sis_parametros where nom_para in ('p_modulo_proveedor')) then i.nombre_tepro
when  a.ide_copag = (select cast(valor_para as integer) from sis_parametros where nom_para in ('p_modulo_empleado')) then  'empleado' end ) as TITULAR,
a.ide_prtra,f.nro_certificacion_prcer,f.detalle_prcer,f.fecha_prcer,d.ide_prcla,d.descripcion_clasificador_prcla,e.codigo_prfup,h.valor_certificado_prpoc




from pre_tramite a
left join pre_poa_tramite b on b.ide_prtra=a.ide_prtra
left join pre_poa c on c.ide_prpoa=b.ide_prpoa
left join pre_poa_certificacion h on h.ide_prpoc=b.ide_prpoc
left join pre_certificacion f on f.ide_prcer=a.ide_prcer
left join pre_clasificador d on d.ide_prcla=c.ide_prcla
left join pre_funcion_programa e on e.ide_prfup=c.ide_prfup
left join cont_parametros_general g on g.ide_copag=a.ide_copag
left join tes_proveedor  i on i.ide_tepro=a.ide_tepro
left join gen_empleados_departamento_par j on j.ide_geedp=a.ide_geedp]]>
	</queryString>
