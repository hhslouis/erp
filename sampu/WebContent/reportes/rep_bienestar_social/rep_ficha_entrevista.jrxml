	<queryString>
		<![CDATA[SELECT d.ide_gtemp,d.EMPLEADO,to_char(d.fecha_nacimiento_gtemp,'dd "de " fmmonth " de " yyyy') as fecha_nacimiento,to_char(d.fecha_ingreso_gtemp,'dd "de " fmmonth " de " yyyy') as fecha_ingreso,
d.path_foto_gtemp,b.motivo_desvinculacion,c.detalle_plan,a.fecha_entrevista_bient,f.lugar_nacimiento,g.lugar_domicilio,e.direccion,a.observaciones_bient
FROM bis_entrevista a
left join(
SELECT ide_bimoe,detalle_bimoe  as motivo_desvinculacion FROM bis_motivo_entrevista
)b on b.ide_bimoe=a.ide_bimoe
left join(
SELECT ide_biple,detalle_biple as detalle_plan FROM bis_plan_entrevista
)c on c.ide_biple=a.ide_biple
left join(
SELECT ide_gtemp,apellido_paterno_gtemp || ' ' ||apellido_materno_gtemp || ' ' ||
primer_nombre_gtemp || ' ' || segundo_nombre_gtemp AS EMPLEADO,
fecha_nacimiento_gtemp,fecha_ingreso_gtemp,path_foto_gtemp,ide_gedip
FROM gth_empleado
)d on d.ide_gtemp=a.ide_gtemp
left join(
SELECT ide_gtdir,ide_gedip,ide_gtemp,detalle_gtdir as direccion FROM gth_direccion where ide_gtdir in(
SELECT max(ide_gtdir) FROM gth_direccion where activo_gtdir=1 and ide_gtemp=$P{ide_gtemp}
group by ide_gtemp)
)e on e.ide_gtemp=d.ide_gtemp
left join(
SELECT ide_gedip,detalle_gedip as lugar_nacimiento FROM gen_division_politica
)f on f.ide_gedip=d.ide_gedip
left join(
SELECT ide_gedip,detalle_gedip as lugar_domicilio FROM gen_division_politica
)g on g.ide_gedip=e.ide_gedip
where a.ide_bient=$P{ide_bient}]]>
	</queryString>
